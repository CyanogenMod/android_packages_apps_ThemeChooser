[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Cyngn Developers
[access "refs/heads/*"]
	push = group Cyngn Developers
	push = group Project Owners
	label-Code-Review = -2..+2 group Cyngn Developers
	label-Code-Review = -2..+2 group Project Owners
	label-Verified = -1..+1 group Cyngn Developers
	label-Verified = -1..+1 group Project Owners
